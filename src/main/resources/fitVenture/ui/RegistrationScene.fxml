<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fitVenture.ui.RegistrationController">
   <children>
      <TextField fx:id="username" layoutX="857.0" layoutY="306.0" promptText="username" />
      <TextField fx:id="password" layoutX="857.0" layoutY="336.0" promptText="password" />
      <Button layoutX="784.0" layoutY="388.0" mnemonicParsing="false" onAction="#register" prefHeight="45.0" prefWidth="230.0" text="Register" />
      <Label layoutX="785.0" layoutY="311.0" prefHeight="17.0" prefWidth="69.0" text="Username:" />
      <Label layoutX="785.0" layoutY="342.0" prefHeight="17.0" prefWidth="69.0" text="Password:" />
      <Label fx:id="errorLabel" layoutX="790.0" layoutY="447.0" prefHeight="17.0" prefWidth="221.0" text="Invalid username or password. Try again" visible="false"/>
   </children>
</AnchorPane>
